<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:lang="clr-namespace:PTMobile.Resources.Languages"
             x:Class="PTMobile.MainPage"
             Title="PTMobile - MainPage"
             BackgroundColor="{AppThemeBinding Light=White,Dark=Black}"
             >

    <StackLayout VerticalOptions="Center">

        <Image HorizontalOptions="Center" Opacity="0.4"  Source="logo.png" WidthRequest="140"></Image>
        <Button x:Name="PruebaNotificaciones" Text="Notificacion" Clicked="PruebaNotificaciones_Clicked"/>

    </StackLayout>
</ContentPage>